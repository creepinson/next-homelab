{
    "extends": ["prettier", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "plugins": ["prettier"],
    "rules": {
        "@typescript-eslint/explicit-module-boundary-types": "off",
        "prettier/prettier": "error"
    }
}
